{
  "name": "thing-people-list",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "gen-api-client": "TS_POST_PROCESS_FILE='prettier --write' openapi-generator-cli generate -g typescript-fetch -i ../../target/openapi.json -o src/apiClient/ -t codegen-templates/openapi-typescript-fetch --enable-post-process-file",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "npm run gen-api-client"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.13",
    "vee-validate": "^4.5.10",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  }
}
